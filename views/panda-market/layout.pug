doctype html
html(lang="en")
    head
        meta(charset="utf-8")
        |
        |
        meta(http-equiv="X-UA-Compatible" content="IE=edge")
        meta(name="viewport" content="width=device-width, initial-scale=1")
        title All services
        link(href="https://fonts.googleapis.com/css?family=Open+Sans:400,600&amp;subset=latin-ext" rel="stylesheet")
        link(href="/css/bootstrap.min.css" rel="stylesheet")
        link(href="/css/font-awesome.min.css" rel="stylesheet")
        link(href="/style.css" rel="stylesheet")
        script(type="text/javascript" src="/js/web3.js")
        script(type="text/javascript" src="/js/uport-connect.min.js")
        <!--[if lt IE 9]>
        script(src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js")
        script(src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js")
        <![endif]-->
    body
        header(class="site-header")
            div.top
                div.container
                    div.row
                        div.col-sm-6
                            p All services
                        div.col-sm-6

            nav.navbar.navbar-default
                div.container
                    a(href="/" class="navbar-brand")
                        img(src="img/pm-logo.png" alt="")
                    div#bs-navbar-collapse(class="collapse navbar-collapse")
                        ul(class="nav navbar-nav main-navbar-nav")
                            li.active
                                a(href="/" title="") Home

        // END MAIN NAVIGATION

        main(class="site-main category-main")
            block content

        input(type="hidden" class="form-control" id='serviceUportId' name='serviceUportId' value=serviceUportId)
        input(type="hidden" class="form-control" id='serviceUportApp' name='serviceUportApp'  value=serviceUportApp)

        footer(class="site-footer")
            div(class="container")
                div(class="row")
                    div(class="col-md-3 col-sm-6 col-xs-12 fbox")
                        h4 Shopping with us
                        ul(class="list-inline")
                            li
                                a(href="#")
                                    i(class="fa fa-facebook")
                            li
                                a(href="#")
                                    i(class="fa fa-twitter")
                            li
                                a(href="#")
                                    i(class="fa fa-linkedin")


            div#copyright
                div(class="container")
                    div(class="row")
                        div(class="col-md-4")
                            p(class="pull-left") &copy; Panda Market
                        div(class="col-md-8")
                            ul(class="list-inline navbar-right")


        script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js")
        script(src="/js/bootstrap.min.js")
        script(type="text/javascript" src="/js/uport.js")
